{
  "expo": {
    "name": "StableFlow",
    "slug": "StableFlow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Behövs för att ta bilder på hagar och stallmiljö.",
        "NSPhotoLibraryUsageDescription": "Behövs för att välja bilder till hagar."
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/logo.png",
      "name": "StableFlow",
      "shortName": "StableFlow",
      "themeColor": "#2D6CF6",
      "backgroundColor": "#F8FAFD",
      "display": "standalone",
      "startUrl": ".",
      "lang": "sv",
      "orientation": "portrait"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
